{
  "name": "chika-next",
  "description": "Landing page for Chika bot.",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "keywords": [
    "chika",
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "dotenv -e .env -- next",
    "build": "dotenv -e .env -- next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "gen": "graphql-codegen -r dotenv/config"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@chakra-ui/icons": "1.0.13",
    "@chakra-ui/react": "1.6.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/open-sans": "4.4.5",
    "@fontsource/rubik": "4.4.5",
    "framer-motion": "^4",
    "graphql": "15.5.1",
    "next": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.3",
    "@graphql-codegen/typescript-operations": "1.18.2",
    "@graphql-codegen/typescript-react-apollo": "2.2.7",
    "@types/node": "14.17.3",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.7",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-next": "11.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "typescript": "4.3.4"
  }
}
